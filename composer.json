{
    "name": "naif/toggle",
    "description": "A Laravel Nova toggle switch field for Nova 3",
    "keywords": [
        "laravel",
        "nova3",
        "field",
        "fields",
        "toggle",
        "switch",
        "toggle-switch"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Naif Alshaye",
            "email": "naif@naif.io",
            "homepage": "https://www.naif.io"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\Toggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\Toggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
